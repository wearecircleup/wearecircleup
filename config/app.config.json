{
  "app": {
    "name": "CircleUp",
    "version": "2.0.0",
    "environment": "${APP_ENV}",
    "baseUrl": "${BASE_URL}"
  },
  "features": {
    "auth": true,
    "presentations": true,
    "analytics": false
  },
  "limits": {
    "maxSlidesPerPresentation": 50,
    "maxPresentationsPerDay": 5,
    "maxDescriptionLength": 5000
  }
}
